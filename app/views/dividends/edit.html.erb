<h2>Edit Dividend for <%= full_ticker_symbol(@dividend.stock) %></h2>

<%= render partial: 'shared/errors', locals: { item: @dividend } if @dividend.errors.any? %>

<%= form_for([@stock, @dividend]) do |f| %>
  <%= render partial: 'form', locals: { f: f } %>

  <%= f.submit 'Edit Dividend' %>
<% end %>